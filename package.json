{
  "name": "openweather-vuejs",
  "version": "1.0.0",
  "description": "Using Vue.js and OpenWeatherMap to render weather data for the selected city",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test mocha-webpack --require ./src/js/test/setup.js ./src/js/test/**/*.spec.js",
    "watch": "cross-env NODE_ENV=test mocha-webpack --require ./src/js/test/setup.js ./src/js/test/**/*.spec.js --watch"
  },
  "author": "Panayiotis “pvgr.eu” Velisarakos",
  "dependencies": {
    "axios": "^0.18.0",
    "foundation-sites": "^6.4.4-rc1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "moxios": "^0.4.0",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.15",
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.5.0",
    "expect": "^22.4.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^1.4.5",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "vue-loader": "^9.9.5",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-node-externals": "^1.7.2"
  }
}
